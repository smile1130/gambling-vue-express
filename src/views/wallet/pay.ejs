        $('.submit').click(function (e) {
            e.preventDefault();
            let id = $(this).attr('id');
            let utr = $('input:eq(0)').val().trim();;
            console.log("utr" + utr);
            $.ajax({
                type: "POST",
                url: "/api/webapi/recharge",
                data: {
                    type: 'submit',
                    typeid: id,
                    utr: utr,
                },
                dataType: "json",
                success: function (response) {
                    if (response.status == true) {
                        $("#result").css("display", "");
                        alert(response.message);
